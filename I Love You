import turtle

t = turtle.Turtle()
t.speed(1)
t.color("red")
t.begin_fill()

t.left(140)
t.forward(113)
for _ in range(200):
    t.right(1)
    t.forward(1)
t.left(120)
for _ in range(200):
    t.right(1)
    t.forward(1)
t.forward(112)
t.end_fill()

t.up()
t.setpos(-68, 95)
t.color("white")
t.write("I love you", font=("Arial", 18, "bold"))
t.hideturtle()

turtle.done()

